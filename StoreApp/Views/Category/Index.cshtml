@model IEnumerable<Category>

<div class="text-center my-3">
    <h6 class="display-6">
        Categories
    </h6>
</div>

<div class="list-group">
    @foreach (var cat in Model)
    {
        <a href="@Url.Action("Index", "Product", new { categoryId = cat.CategoryId })" class="list-group-item list-group-item-action">
            @cat.CategoryName
        </a>
    }
</div>
